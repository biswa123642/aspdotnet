<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<contentSearch>
			<indexConfigurations>
				<CGPConfiguration>
					<documentOptions type="Sitecore.ContentSearch.SolrProvider.SolrDocumentBuilderOptions, Sitecore.ContentSearch.SolrProvider">
						<fields hint="raw:AddComputedIndexField">
							<field fieldName="contenttype" returnType="string" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.ContentType, CGP.Foundation.Search</field>
							<field fieldName="computedimage" returnType="text" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.ComputedImage, CGP.Foundation.Search</field>
							<field fieldName="secondarycategorylist" returnType="text" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.ProductGrouping, CGP.Foundation.Search</field>
							<field fieldName="productattributelist" returnType="multiple" indexed="true" multivalued="true">CGP.Foundation.Search.Indexing.ComputedFields.ProductAttributeList, CGP.Foundation.Search</field>
							<field fieldName="pageattributelist" returnType="multiple" indexed="true" multivalued="true">CGP.Foundation.Search.Indexing.ComputedFields.PageAttributeList, CGP.Foundation.Search</field>
							<field fieldName="blogtopic" returnType="stringCollection" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.ComputedBlogTopic, CGP.Foundation.Search</field>
							<field fieldName="blogtype" returnType="stringCollection" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.ComputedBlogType, CGP.Foundation.Search</field>
							<field fieldName="contenturl" returnType="text" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.ContentUrl, CGP.Foundation.Search</field>
							<field fieldName="templateid" returnType="string" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.ItemTemplateId, CGP.Foundation.Search</field>
							<field fieldName="customredirectlink" returnType="string" indexed="true" stored="true">CGP.Foundation.Search.Indexing.ComputedFields.RedirectUrl, CGP.Foundation.Search</field>
						</fields>
					</documentOptions>
				</CGPConfiguration>
			</indexConfigurations>
		</contentSearch>
	</sitecore>
</configuration>